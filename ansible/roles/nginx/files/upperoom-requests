server {
    listen 80;
    server_name 50.17.117.96 ec2-50-17-117-96.compute-1.amazonaws.com;
    
    location / {
            return 301 https://$host$request_uri;
            include proxy_params;
            proxy_pass http://127.0.0.1:8000;
        }

    location /static/ {
        alias /var/www/static/;
    }
}

server {
    listen 443 ssl;
    server_name ec2-50-17-117-96.compute-1.amazonaws.com

    # ssl_certificate /etc/letsencrypt/live/www.parcelroyale.com/fullchain.pem;
    # ssl_certificate_key /etc/letsencrypt/live/www.parcelroyale.com/privkey.pem;
    # include /etc/letsencrypt/options-ssl-nginx.conf;
    # ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem;

    location / {
        include proxy_params;
        proxy_pass http://127.0.0.1:8000;
    }

    location /static/ {
        alias /var/www/static/;
    }
}
